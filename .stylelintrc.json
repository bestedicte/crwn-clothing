{
  "extends": [
    "stylelint-config-sass-guidelines"
  ],
  "plugins": [
    "stylelint-scss"
    ],
  "rules": {
    "indentation": "tab",
    "number-leading-zero": null,
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true,
    "order/order": [
      "custom-properties",
      "declarations"
    ],
      "max-nesting-depth": null
  }
}